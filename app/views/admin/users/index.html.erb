<div class="card">
<div class="header">
<h4 class="title">Users Table</h4>
<p class="category">All registred user with roles</p>
</div>
<div class="content table-responsive table-full-width">
<table class="table table-hover table-striped">
<thead>
<th>Username</th>
<th>Email</th>
<th>Created at</th>
<th>Sign in count</th>
<th>Admin</th>
</thead>
<tbody>
<% for user in @users %>
<tr>
<td><%= user.username %></td>
<td><%= user.email %></td>
<td><%= user.created_at.strftime("at %I:%M%p on %m/%d/%Y") %></td>
<td><%= user.last_sign_in_at.strftime("at %I:%M%p on %m/%d/%Y") %></td>
<td><%= user.admin %></td>
</tr> 
<% end %>                                     
</tbody>
</table>

</div>
</div>
</div>